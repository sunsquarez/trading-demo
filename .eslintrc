{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "parser": "@babel/eslint-parser",
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "warn"
  }
}
